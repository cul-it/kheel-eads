    <archdesc level="collection">
        <did>
            <head id="a1">DESCRIPTIVE SUMMARY</head>
            <unittitle label="Title:" encodinganalog="MARC 245$a">Teachers Union of the City of New
                York Additional Records, <unitdate encodinganalog="MARC 245$f">1931-1937</unitdate>
            </unittitle>
            <unitid label="Collection Number:">5927</unitid>
