    <archdesc level="collection">
        <did>
            <head id="a1">DESCRIPTIVE SUMMARY</head>
            <unittitle label="Title:" encodinganalog="MARC 245$a">Farnsworth Manufacturing Records,
                    <unitdate encodinganalog="MARC 245$f">1931-1961</unitdate>
            </unittitle>
            <unitid label="Collection Number:">6636</unitid>
            <origination label="Creator:">
